{
	"Bot": {
		"Admins": [ "Snowflake", "Snowflake" ]
	},
	"Discord": {
		"Token": "my-authentication-key-here",
		"Prefix": {
			"Prefixes": [ ">>", "!", ">", "^" ],
			"Mention": true
		},
		"ClientOptions": {
			"fetchAllMembers": true,
			"ws": {
				"large_threshold": 50
			}
		}
	},
	"Logging": {
		"Level": "info",
		"Prefix": "lexi-",
		"DatePattern": "yyyy-M-d",
		"Container": "logs",
		"JSON": false
	},
	"MySQL": {
		"Hostname": "database.example.com",
		"Authentication": {
			"User": "mydbuser",
			"Password": "mypassword"
		},
		"Database": "lexi"
	}
}